{
  "expo": {
    "name": "ETH BioDex",
    "slug": "ETH-Biodex",
    "privacy": "public",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.8",
    "orientation": "default",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/BioDex_logo_name_whitebackground.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "ch.ethz.biodex",
      "buildNumber": "1.0.8",
      "supportsTablet": true
    },
    "android": {
      "package": "ch.ethz.biodex",
      "versionCode": 1,
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "CAMERA",
        "WRITE_EXTERNAL_STORAGE",
        "READ_EXTERNAL_STORAGE"
      ]
    }
  }
}
